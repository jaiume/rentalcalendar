{
    "name": "jamiedev/rentalcalendar",
    "description": "Rental calendar Slim app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Dev",
            "email": "jamie@example.com"
        }
    ],
    "require": {
        "slim/slim": "^4.15",
        "slim/psr7": "^1.8",
        "php-di/php-di": "^7.1",
        "slim/twig-view": "^3.4",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    }
}
